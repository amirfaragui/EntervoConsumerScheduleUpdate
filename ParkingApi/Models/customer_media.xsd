    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Version Type                                                      * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="version" type="gsph-cust:versionType">
	<annotation>
	    <documentation>
		 This element is used as the key version information of the REST interface.
	    </documentation>
	</annotation>
    </element>
    <complexType name="versionType">
      <sequence>
          <element name="version" type="string" minOccurs="0" maxOccurs="1"/>
      </sequence>
    </complexType>

    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Resources Type                                                    * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="resources" type="gsph-cust:resourcesType">
	<annotation>
	    <documentation>
		This element provides a dictionary of all available application resources and their URIs.
	    </documentation>
	</annotation>
    </element>
    <complexType name="resourcesType">
      <sequence>
          <element name="resource" type="string" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>

   <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * List of Contracts (collection of contract)                        * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contracts" type="gsph-cust:contractsType"/>
    
    <complexType name="contractsType">
      <annotation>
        <documentation>Defines a list of contract keys</documentation>
      </annotation>
      <sequence>
          <element name="contract" type="gsph-cust:contractType" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Contract Type                                                     * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contract" type="gsph-cust:contractType">
        <annotation>
            <documentation>This element is used as the key contract information. This element is sent
                when querying lists in order to avoid sending whole datasets
            </documentation>
        </annotation>
    </element>
    <complexType name="contractType">
      <sequence>
          <element name="id" type="gsph-cust:contractIdType" minOccurs="1" maxOccurs="1">
            <annotation>
              <documentation>Unique identifier of a contract.
                The range is between 2 and 49999 for contracts based for a single cell
                and between 50000 and 99999 for contracts based on management level.
              </documentation>
            </annotation>
          </element>
          <element name="cstmrId40" type="gsph-cust:string20" minOccurs="0" maxOccurs="1"/>
          <element name="cstmrIdSAP" type="gsph-cust:string50" minOccurs="0" maxOccurs="1"/>
          <element name="name" type="gsph-cust:string33" minOccurs="0" maxOccurs="1"/>
          <element name="xValidFrom" type="date" minOccurs="0" maxOccurs="1"/>
          <element name="xValidUntil" type="date" minOccurs="0" maxOccurs="1"/>
          <element name="filialId" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
      </sequence>
      <attribute name="href" type="anyURI">
        <annotation>
          <documentation>The URI supplied in this field can be used to query the
            detailed information of a contract
          </documentation>
        </annotation>
      </attribute>
    </complexType>
        
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Contract Detail Type                                              * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contractDetail" type="gsph-cust:contractDetailType">
        <annotation>
            <documentation>
                The contract detail element is the container for further complex elements:

                Contract:             Key contract information
                Person:               Peronal attributes like first name, last name etc.
                Address:              Standard Address
                Invoice Address:      Address where the invoice should be sent
                paymentInfo:          Information about payment details
                counting:             company counting information
                present:              number of present consumersassociated to a contract
            </documentation>
        </annotation>
    </element>
    <complexType name="contractDetailType">
        <sequence>
            <element name="contract" type="gsph-cust:contractType"/>
            <element name="contractAttributes" type="gsph-cust:contractAttributesType" minOccurs="0" maxOccurs="1"/>
            <element name="contractno" type="string" minOccurs="0" maxOccurs="1"/>
            <element name="cntrctId40" type="gsph-cust:string20" minOccurs="0" maxOccurs="1"/>
            <element name="cntrctIdSAP" type="gsph-cust:string50" minOccurs="0" maxOccurs="1"/>
            <element name="productId40" type="gsph-cust:string20" minOccurs="0" maxOccurs="1"/>
            <element name="person" type="gsph-cust:personAttributesType" minOccurs="0" maxOccurs="1"/>
            <element name="stdAddr" type="gsph-cust:addressType" minOccurs="0" maxOccurs="1"/>
            <element name="deliveryPerson" type="gsph-cust:personAttributesType" minOccurs="0" maxOccurs="1"/>
            <element name="deliveryAddr" type="gsph-cust:addressType" minOccurs="0" maxOccurs="1"/>
            <element name="paymentInfo" type="gsph-cust:paymentInfoType" minOccurs="0" maxOccurs="unbounded"/>
            <element name="counting" type="gsph-cust:counterType" minOccurs="0" maxOccurs="1"/>
            <element name="present" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
            <element name="status" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
            <element name="delete" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
            <element name="memo" type="gsph-cust:string51" minOccurs="0" maxOccurs="1"/>
            <element name="userfield1" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="userfield2" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="userfield3" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="invoicegroup" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
            <element name="loyalty" type="gsph-cust:loyaltyType" minOccurs="0" maxOccurs="1"/>
            <element name="pooling" type="gsph-cust:poolingType" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Contract attributte Type                                                     * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contractAttributes" type="gsph-cust:contractAttributesType"/>        
        
    <complexType name="contractAttributesType">
      <sequence>  
        <element name="autoBlockDays" minOccurs="0" maxOccurs="1" type="unsignedShort"/>
        <element name="prePayment" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
        <element name="discountCash" minOccurs="0" maxOccurs="1" type="unsignedShort"/>
        <element name="discount" minOccurs="0" maxOccurs="1" type="gsph-cust:discountType"/>
        <element name="discountValue" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
        <element name="feeType" minOccurs="0" maxOccurs="1" type="gsph-cust:feeTypeType"/>
        <element name="flatFee" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
        <element name="vat" minOccurs="0" maxOccurs="1" type="gsph-cust:vatType"/>
        <element name="flatFeeCalcUntil" minOccurs="0" maxOccurs="1" type="date"/>
        <element name="flatFeeType" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeTypeType"/>
        <element name="flatFeeCalc" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeCalcType"/>
        <element name="flatFeeFirstMonth" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeFirstMonthType"/>
        <element name="flatFeeLastMonth" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeLastMonthType"/>
      </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * List of Contractdetails (collection of contract details)          * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contractDetails" type="gsph-cust:contractDetailsType"/>

    <complexType name="contractDetailsType">
      <annotation>
        <documentation>Defines a list of contract details</documentation>
      </annotation>
      <sequence>
          <element name="contractDetail" type="gsph-cust:contractDetailType" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>

    <!-- ************************************************************************** -->
    <!-- *                                                                        * -->
    <!-- * List of Contractfreefacilities (collection of contract free facilities * -->
    <!-- *                                                                        * -->
    <!-- ************************************************************************** -->
    <element name="contractFreeFacilities" type="gsph-cust:contractFreeFacilitiesType"/>

    <complexType name="contractFreeFacilitiesType">
      <annotation>
        <documentation>Defines a list of contract free facilities</documentation>
      </annotation>
      <sequence>
          <element name="contractFreeFacility" type="gsph-cust:contractFreeFacilityType" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>

    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Contract free facilitie Type                                     * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="contractFreeFacility" type="gsph-cust:contractFreeFacilityType">
        <annotation>
            <documentation>
            The contract freefacility element is the container for further complex elements:

            </documentation>
        </annotation>
    </element>
    <complexType name="contractFreeFacilityType">
        <sequence>
            <element name="xValidFrom" type="date"/>
            <element name="xValidUntil" type="date"/>
            <element name="facilityid" type="unsignedShort"/>
        </sequence>
    </complexType>

    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Consumer Type                                                     * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="consumer" type="gsph-cust:consumerType">
        <annotation>
            <documentation>The consumer type comprises the key information of a person
                who accesses a car park. The key information comprises the consumer id, the
                id of the contract the consumer is related to and the main personal attributes
                like name.
            </documentation>
        </annotation>
    </element>
    <complexType name="consumerType">
        <sequence>
            <element name="id" type="gsph-cust:consumerIdType" minOccurs="1" maxOccurs="1">
              <annotation>
                <documentation>
                    This is the unique identifier within the contract the consumer is belonging to.
                    When creating a consumer the system will provide this number automatically (next free number).
                    The range is between 1 and 99.998.
                </documentation>
              </annotation>
            </element>
            <element name="contractid" type="gsph-cust:contractIdType" minOccurs="1" maxOccurs="1">
              <annotation>
                <documentation>
                    This is the reference to the id of the contract to which the consumer belongs to.
                    Contract id together with the consumer id are unique in the system.
                </documentation>
              </annotation>
            </element>
            <element name="cnsmrId40" type="gsph-cust:string20" minOccurs="0" maxOccurs="1"/>
            <element name="name" type="string" minOccurs="0" maxOccurs="1"/>
            <element name="xValidFrom" type="date" minOccurs="0" maxOccurs="1"/>
            <element name="xValidUntil" type="date" minOccurs="0" maxOccurs="1"/>
            <element name="filialId" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
        </sequence>
        <attribute name="href" type="anyURI"/>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * consumer detail                                                   * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="consumerDetail" type="gsph-cust:consumerDetailType"/>
    <complexType name="consumerDetailType">
        <sequence>
            <element name="consumer" type="gsph-cust:consumerType" minOccurs="0" maxOccurs="1"/>
            <element name="person" type="gsph-cust:personAttributesType" minOccurs="0" maxOccurs="1"/>
            <element name="firstName" type="gsph-cust:string33" minOccurs="0" maxOccurs="1"/>
            <element name="surname" type="gsph-cust:string31" minOccurs="0" maxOccurs="1"/>
            <element name="identification" type="gsph-cust:identificationType" minOccurs="1" maxOccurs="unbounded"/>
            <element name="consumerAtributes" type="gsph-cust:consumerAtributesType" minOccurs="0" maxOccurs="1"/>  
            <element name="billingData" type="gsph-cust:billingDataType" minOccurs="0" maxOccurs="1"/>              
            <element name="displayText" type="string" minOccurs="0" maxOccurs="1"/>
            <element name="currencyRef" type="string" minOccurs="0" maxOccurs="1">
              <annotation>
                <documentation>Currently not supported</documentation>
              </annotation>
            </element>
            <element name="limit" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
            <element name="limitPerDay" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
            <element name="transactionPerDay" type="unsignedLong" minOccurs="0" maxOccurs="1"><annotation>
                <documentation>Currently not supported</documentation>
              </annotation>
            </element>
            <element name="memo" type="gsph-cust:string51" minOccurs="0" maxOccurs="1">
              <annotation>
                <documentation>Any additional information can be entered here.
                    The memo has no operational effect.
                    The information is displayed in the customer administration user interface
                </documentation>
              </annotation>
            </element>
            <element name="status" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
            <element name="delete" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
            <element name="ignorePresence" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
            <element name="lpn1" type="gsph-cust:string15" minOccurs="0" maxOccurs="1"/>
            <element name="lpn2" type="gsph-cust:string15" minOccurs="0" maxOccurs="1"/>
            <element name="lpn3" type="gsph-cust:string15" minOccurs="0" maxOccurs="1"/>
            <element name="userfield1" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="userfield2" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="userfield3" type="gsph-cust:string128" minOccurs="0" maxOccurs="1"/>
            <element name="loyalty" type="gsph-cust:loyaltyType" minOccurs="0" maxOccurs="1"/>
            <element name="identifiers" minOccurs="0" maxOccurs="1" type="gsph-cust:identifiersType"/>
        </sequence>
    </complexType>

    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * List of Consumers (Collection of Consumer)                        * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="consumers" type="gsph-cust:consumersType"/>
    <complexType name="consumersType">
        <sequence>
            <element name="consumer" type="gsph-cust:consumerType" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * List of Consumer Details                                          * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="consumerDetails" type="gsph-cust:consumerDetailsType"/>
    <complexType name="consumerDetailsType">
        <sequence>
            <element name="consumerDetail" type="gsph-cust:consumerDetailType" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>
    
   <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Contract attributte Type                                           * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="consumerAtributes" type="gsph-cust:consumerAtributesType"/>        
        
    <complexType name="consumerAtributesType">
      <sequence>  
          <element name="productionDate" minOccurs="0" maxOccurs="1" type="date">
            <annotation>
                <documentation>read only</documentation>
            </annotation>
          </element>
          <element name="productionCount" minOccurs="0" maxOccurs="1" type="unsignedShort">
            <annotation>
                <documentation>read only</documentation>
            </annotation>
          </element>
          <element name="flatFeeFirstCharge" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeFirstMonthType"/>
          <element name="flatFeeLastCharge" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeLastMonthType"/>
          <element name="flatFeeCalc" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeCalcType"/>
          <element name="flatFeeCalcUntil" minOccurs="0" maxOccurs="1" type="date"/>
          <element name="individualInvoicing" minOccurs="0" maxOccurs="1" type="unsignedShort"/>
		  <element name="flatRateAmt" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
          <element name="flatFeeTax" minOccurs="0" maxOccurs="1" type="gsph-cust:vatType"/>
          <element name="flatFeeType" minOccurs="0" maxOccurs="1" type="gsph-cust:flatFeeTypeType"/>
          
          <element name="invoiceType" minOccurs="0" maxOccurs="1" type="gsph-cust:invoiceTypeType"/>
          <element name="minInvoice" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
          <element name="maxInvoice" minOccurs="0" maxOccurs="1" type="unsignedLong"/>
          <element name="creditEntry" minOccurs="0" maxOccurs="1" type="unsignedLong"/>       
      </sequence>
    </complexType>   
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Identification                                                    * -->
    <!-- *                                                                   * -->
    <!-- * In terms of the "old" system this is a "Teilnehmer"               * -->
    <!-- * In terms of the "new" system this could be a e.g Credit Card      * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <complexType name="identificationType">
      <sequence>
          <element name="id" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
          <element name="ptcptType" type="integer" minOccurs="0" maxOccurs="1"/>
          <element name="cardno" type="gsph-cust:string9" minOccurs="0" maxOccurs="1"/>
          <element name="cardclass" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
          <element name="identificationType" type="integer" minOccurs="0" maxOccurs="1"/>
          <element name="validFrom" type="date" minOccurs="0" maxOccurs="1"/>
          <element name="validUntil" type="date" minOccurs="0" maxOccurs="1"/>
          <element name="usageProfile" type="gsph-cust:usageProfileType" minOccurs="0" maxOccurs="1"/>
          <element name="admission" type="gsph-cust:admissionType" minOccurs="0" maxOccurs="1"/>
          <element name="ignorePresence" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
          <element name="present" type="boolean" minOccurs="0" maxOccurs="1"/>
          <element name="status" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
          <element name="ptcptGrpNo" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
	  <element name="chrgOvdrftAcct" type="unsignedShort" minOccurs="0" maxOccurs="1"/>
      </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Consumers`s identifiers                                           * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="identifiers" type="gsph-cust:identifiersType"/>
    
    <complexType name="identifiersType">
      <sequence>  
        <element name="identifier" type="gsph-cust:identifierType" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
    
    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Identifier data                                                   * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <complexType name="identifierType">
        <annotation>
            <documentation>ReadOnly params - card number and license plate </documentation>
        </annotation>
        <sequence>
            <element name="ptcptId" type="unsignedLong" minOccurs="0" maxOccurs="1"/>
            <element name="cardNo" type="gsph-cust:string9" minOccurs="0" maxOccurs="1"/>
            <element name="licensePlate" type="gsph-cust:string15" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <!-- ********************************************************************* -->
    <!-- *                                                                   * -->
    <!-- * Billing data                                                      * -->
    <!-- *                                                                   * -->
    <!-- ********************************************************************* -->
    <element name="billingData" type="gsph-cust:billingDataType"/>
    <complexType name="billingDataType">
        <annotation>
            <documentation>ReadOnly param of person, adress and payment data for consumer (Shadow invoice) </documentation>
        </annotation>
        <sequence>
            <element name="billingPerson" type="gsph-cust:personAttributesType" minOccurs="1" maxOccurs="1"/>
            <element name="billingAddr" type="gsph-cust:addressType" minOccurs="1" maxOccurs="1"/>
            <element name="paymentInfo" type="gsph-cust:paymentInfoType" minOccurs="0" maxOccurs="1"/>
